{
  "name": "joanna-portfolio-2023",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-tailwind/react": "^2.1.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{html,js,jsx}\"",
    "watch:storybook": "npx tailwindcss -i ./src/App.css -o ./public/output.css --watch",
    "build:storybook": "npx tailwindcss -i ./src/App.css -o ./public/output.css",
    "storybook": "watch:storybook & storybook dev -p 6006",
    "build-storybook": "build:storybook && storybook build",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.4.4",
    "@storybook/addon-interactions": "^7.4.4",
    "@storybook/addon-links": "^7.4.4",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.4",
    "@storybook/preset-create-react-app": "^7.4.4",
    "@storybook/react": "^7.4.4",
    "@storybook/react-webpack5": "^7.4.4",
    "@storybook/testing-library": "^0.2.1",
    "autoprefixer": "^10.4.15",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint-plugin-storybook": "^0.6.14",
    "prop-types": "^15.8.1",
    "storybook": "^7.4.4",
    "webpack": "^5.88.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3"
  },
  "lint-staged": {
    "*.{html,js,jsx}": "prettier --write"
  }
}
